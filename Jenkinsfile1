pipeline {
  agent any
   stages {
   stage('build'){
   steps{
      sh 'docker build my-image . '
   }
   }
      stage('Test') {
              steps {
                 
                     sh 'docker run -it  my-image'
                    echo "docker image tested"
                    }
           }
          stage('Deploy') {
              steps {
              sh 'docker run -it  my-image'
                    sh 'docker push naveenkumaraluthuri Dockerfile'
                  }
            }
  }
 }
