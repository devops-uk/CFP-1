pipeline {
  agent any
   stages {
      stage('build') {
          steps {
                sh 'docker build -t Dockerfile'
                
                }
      }    
        stage('Test') {
              steps {
                     sh 'docker build -t Dockerfile'
                     sh 'docker run  Dockerfile'
                    echo "docker image tested"
                    }
           }
          stage('Deploy') {
              steps {
              sh 'docker run  Dockerfile'
                    sh 'sudo docker push naveenkumaraluthuri Dockerfile'
                  }
            }
  }
 }
